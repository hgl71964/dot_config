# ssh config file

Host ec1
  HostName ec2-54-74-0-183.eu-west-1.compute.amazonaws.com
  User ubuntu
  IdentityFile ~/.ssh/Ireland_ec1.cer
Host ec2
  HostName ec2-18-203-250-152.eu-west-1.compute.amazonaws.com
  User ubuntu
  IdentityFile ~/.ssh/Ireland_ec1.cer
  # Port *
Host cam_cl
  HostName ely.cl.cam.ac.uk
  User gh512
  GSSAPIAuthentication yes
  # Port *
Host cam_linux_cl
  HostName linux.cl.ds.cam.ac.uk
  User gh512
  # password -> Raven password
Host cam_linux_public
  HostName linux.ds.cam.ac.uk
  User gh512
  # password -> Raven password
Host *
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_rsa
